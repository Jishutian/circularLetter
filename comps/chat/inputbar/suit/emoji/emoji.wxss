.emoji_list {
	margin-top: 30px;
	width: 100%;
	height: 145px;
	background-color: #dddddd;
	padding-top: 10px;
	padding-left: 3%;
	display: none;
}

.showEmoji {
  position: fixed;
  left:0;
  bottom:104rpx;
	margin-top: 30px;
	width:750rpx;
  box-sizing:border-box;
	height: 145px;
	background-color: #dddddd;
	padding-top: 10px;
	padding-left: 3%;
	display: block;
  z-index:10;
}

.emoji_list image,
.showEmoji image {
	width: 26px;
	height: 26px;
	margin: 5px 2%;
}

.emoji {
	width: 26px;
	height: 26px;
	margin: 0 0;
}

.emoji_item {
	display: flex;
	justify-content: space-around;
	margin-right: 20px;
}
